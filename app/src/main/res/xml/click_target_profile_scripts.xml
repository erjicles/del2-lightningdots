<?xml version="1.0" encoding="utf-8"?>
<ClickTargetProfileScripts>

    <ClickTargetProfileScript
        level="1">
        <ClickTargetProfile/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="2">
        <ClickTargetProfile
            targetCanChangeRadius="true"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="3">
        <ClickTargetProfile
            targetCanRandomlyChangeRadius="true"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="4">
        <ClickTargetProfile
            targetCanChangeSpeed="true"
            maximumTargetSpeedMultiplier="3.0"
            maximumTargetSpeedChangeMultiplier="1.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="5">
        <ClickTargetProfile
            initialTargetSpeedMultiplier="2.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="6"
        scriptTransitionMode="CYCLE"
        scriptTransitionInterval="REGULAR">
        <ClickTargetProfile
            scriptTransitionValue="1500.0"
            scriptTransitionContinuousDirection="CONTINUOUS"
            scriptTransitionContinuousRadius="CONTINUOUS"
            targetCanChangeRadius="true"/>
        <ClickTargetProfile
            scriptTransitionValue="1500.0"
            scriptTransitionContinuousDirection="CONTINUOUS"
            scriptTransitionContinuousRadius="CONTINUOUS"
            initialTargetSpeedMultiplier="2.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="7">
        <ClickTargetProfile
            initialTargetRadiusMultiplier="0.5"
            initialTargetSpeedMultiplier="1.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="8">
        <ClickTargetProfile
            targetCanChangePosition="false"
            targetCanRandomlyChangePosition="true"
            probabilityOfRandomPositionChangePerSecond="0.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="9">
        <ClickTargetProfile
            initialTargetRadiusMultiplier="0.8"
            minimumTargetRadiusMultiplier="0.8"
            targetCanChangeDirection="true"
            targetCanRandomlyChangeDDirection="true"
            initialTargetSpeedMultiplier="1.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="10">
        <ClickTargetProfile
            initialTargetSpeedMultiplier="2.0"
            initialTargetRadiusMultiplier="0.8"
            minimumTargetRadiusMultiplier="0.4"
            targetCanChangeRadius="true"
            maximumTargetDRadiusMultiplier="1.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="11">
        <ClickTargetProfile
            initialTargetSpeedMultiplier="1.5"
            targetCanRandomlyChangeDirection="true"
            probabilityOfRandomDirectionChangePerSecond="0.50"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="12"
        scriptTransitionMode="CYCLE"
        scriptTransitionInterval="RANDOM">
        <ClickTargetProfile
            scriptTransitionValue="0.50"
            scriptTransitionContinuousSpeed="CONTINUOUS"
            scriptTransitionContinuousDirection="CONTINUOUS"
            targetCanChangeSpeed="true"
            targetCanChangeRadius="true"/>
        <ClickTargetProfile
            scriptTransitionValue="0.50"
            scriptTransitionContinuousSpeed="CONTINUOUS"
            scriptTransitionContinuousDirection="CONTINUOUS"
            targetCanChangeDirection="true"
            targetCanRandomlyChangeDirection="true"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="13"
        scriptTransitionMode="RANDOM"
        scriptTransitionInterval="REGULAR">
        <ClickTargetProfile
            scriptTransitionValue="1500.0"
            scriptTransitionContinuousRadius="DISCONTINUOUS"
            scriptTransitionContinuousSpeed="DISCONTINUOUS"
            initialTargetRadiusMultiplier="0.50"
            initialTargetSpeedMultiplier="1.75"/>
        <ClickTargetProfile
            scriptTransitionValue="1500.0"
            scriptTransitionContinuousRadius="DISCONTINUOUS"
            scriptTransitionContinuousSpeed="DISCONTINUOUS"
            initialTargetRadiusMultiplier="0.75"
            targetCanRandomlyChangeRadius="true"
            initialTargetSpeedMultiplier="1.5"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="14">
        <ClickTargetProfile
            initialTargetRadiusMultiplier="0.75"
            targetCanRandomlyChangePosition="true"
            probabilityOfRandomPositionChangePerSecond="0.50"
            targetCanRandomlyChangeRadius="true"
            probabilityOfRandomRadiusChangePerSecond="0.50"/>
    </ClickTargetProfileScript>

    <ClickTargetProfileScript
        level="15">
        <ClickTargetProfile
            targetCanRandomlyChangeSpeed="true"
            probabilityOfRandomSpeedChangePerSecond="0.65"
            targetCanRandomlyChangeDirection="true"
            probabilityOfRandomDirectionChangePerSecond="0.50"
            initialTargetRadiusMultiplier="0.65"/>
    </ClickTargetProfileScript>

</ClickTargetProfileScripts>